        "# combine all together\n",
        "users_data_test = pd.merge(users_events_data_test, users_scores_test,\n",
        "                      on='user_id', \n",
        "                      how='outer').fillna(0)\n",
        "\n",
        "users_data_test = users_data_test.merge(users_steps_tried_test, how='outer').fillna(0)"
